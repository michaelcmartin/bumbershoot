pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
-- shooting gallery
-- michael martin, 2024
function _init()
  target_y=20
  blaster_x=60
  target_x={ 28, 60, 92 }
  shot_x={ }
  shot_y={ }
  score=0
end

function _draw()
  local i
  cls()
  -- ground
  rectfill(0,116,127,127,3)
  -- blaster
  spr(1,blaster_x,108)
  -- targets
  for i=1,#target_x do
    spr(2,target_x[i],target_y)
  end
  -- shots
  for i=1,#shot_x do
    local x,y
    x=shot_x[i]
    y=shot_y[i]
    line(x,y,x,y+2,10)
  end
  -- score and divider
  print("\f7score: \fa",84,0)
  print(score\1000,112,0)
  print((score\100)%10,116,0)
  print((score\10)%10,120,0)
  print(score%10,124,0,10)
  line(0,6,127,6,7)
end

function _update60()
  local i,x,y
  for i=1,#target_x do
    x=target_x[i]
    x-=1
    if x<=-8 then x=128 end
    target_x[i]=x
  end
  if #shot_y > 0 and shot_y[1]<=6 then
    deli(shot_x,1)
    deli(shot_y,1)
  end
  -- check for collisions
  i=1
  while i<=#shot_x do
    y=shot_y[i]
    if y+2>target_y and y<target_y+7 then
      local j
      x=shot_x[i]
      for j=1,3 do
        local tl,tr
        tl=target_x[j]+1
        tr=target_x[j]+7
        if x>=tl and x<=tr then
          score+=1
          deli(shot_x,i)
          deli(shot_y,i)
          i=#shot_x
        end
      end
    end
    i+=1
  end
  y=0
  for i=1,#shot_x do
    shot_y[i] -= 1
    y=shot_y[i]
  end
  if blaster_x > 4 and btn(0) then blaster_x -= 1 end
  if blaster_x < 116 and btn(1) then blaster_x += 1 end
  if target_y > 7 and btn(2) then target_y -= 1 end
  if target_y < 101 and btn(3) then target_y += 1 end
  if y<90 and btn(4) then
    add(shot_x,blaster_x+3)
    add(shot_y,105)
  end
end
